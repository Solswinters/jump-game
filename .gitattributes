# Auto detect text files and normalize line endings to LF
* text=auto eol=lf

# Force binary files to be left untouched
*.png binary
*.jpg binary
*.jpeg binary
*.gif binary
*.ico binary
*.mov binary
*.mp4 binary
*.mp3 binary
*.flv binary
*.fla binary
*.swf binary
*.gz binary
*.zip binary
*.7z binary
*.ttf binary
*.eot binary
*.woff binary
*.woff2 binary
*.otf binary
*.pyc binary
*.pdf binary
*.ez binary
*.bz2 binary
*.swp binary

# Linguist overrides
*.ts linguist-detectable=true
*.tsx linguist-detectable=true
*.js linguist-detectable=true
*.jsx linguist-detectable=true
*.css linguist-detectable=true
